{
  "name": "04-expressrentroomapi",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "node --loader ts-node/esm src/index.ts",
    "start:local-db": "docker compose up -d",
    "start:console-runners": "tsx src/console-runners/index.ts",
    "type-check": "tsc --noEmit --preserveWatchOutput",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest --verbose",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc --project tsconfig.prod.json && tsc-alias -p tsconfig.prod.json"
  },
  "imports": {
    "#*": "./src/*"
  },
  "author": "Sergio Gordillo",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@inquirer/prompts": "^7.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "mongodb": "^6.17.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.0.5",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
